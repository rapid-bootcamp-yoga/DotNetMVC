@model CustomerViewModel

@{
    ViewData["Title"] = "Customer Detail";
}

<h2>Product Detail</h2>
<table class="tab tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Posisi</th>
            <td>@Model.Password</td>
        </tr>
    </tbody>
</table>